 CellChangeTimes->{{3.428440492914269*^9, 3.428440495831313*^9}, {
  3.428540333832511*^9, 3.428540338498201*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"SetDirectory", "[", 
   RowBox[{"ToFileName", "[", 
    RowBox[{"{", 
     RowBox[{
     "$HomeDirectory", ",", 
      "\"\<Projects/articles/topo/ttauto/pubs/braids/code_abridged\>\""}], 
     "}"}], "]"}], "]"}], ";"}]], "Input",
