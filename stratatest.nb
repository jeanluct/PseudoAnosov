 CellChangeTimes->{{3.4444281370424843`*^9, 3.444428137143355*^9}, {
  3.444734938374177*^9, 3.444734938710763*^9}}],

Cell[BoxData[
 RowBox[{"PLehmer", "=", 
  RowBox[{
   SuperscriptBox["x", "10"], "+", 
